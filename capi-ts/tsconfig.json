{
  "include": ["./"],
  "exclude": [],
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "lib": ["dom", "esnext"],
    "importHelpers": false,
    "declaration": true,
    "sourceMap": true,
    "rootDir": "./",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "outDir": "./"
  }
}
